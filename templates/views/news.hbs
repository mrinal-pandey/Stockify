{{>header}}
<div class="card-group news">
    {{#each news}}
    <div class="card news-card">
        <img src={{this.image}} class="card-img-top">
        <div class="card-body">
            <h5 class="card-title">{{this.title}}</h5>
            <p class="card-text">{{this.text}}</p>
        </div>
        <div class="card-footer">
            {{this.publishedDate}}
        </div>
    </div>
    {{/each}}
</div>
{{>footer}}